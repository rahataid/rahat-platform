{
  "name": "@rahat-os/source",
  "version": "0.0.0",
  "license": "MPL-2.0",
  "scripts": {
    "rahat": "set NODE_ENV=development && npx nx serve rahat",
    "compodoc": "npx compodoc -p tsconfig.doc.json",
    "rahat:prod": "set NODE_ENV=production && npx nx serve rahat",
    "beneficiary": "set NODE_ENV=development && npx nx serve beneficiary",
    "serve:apps": "nx run-many -p rahat beneficiary --targets=serve",
    "add:resource": "nx g @nx/nest:resource --directory=apps/$app/src --crud=true",
    "build:all": "nx run-many --target=build --all --parallel=true --verbose",
    "build:rahat": "nx build rahat",
    "build:beneficiary": "nx build beneficiary",
    "setup:devtools": "bash ./tools/scripts/setupDevTools.sh",
    "bootstrap": "bash ./tools/scripts/local-setup/bootstrap.sh",
    "reset": "bash ./tools/scripts/local-setup/reset.sh",
    "clean": "rm -rf dist node_modules tmp",
    "format": "nx format:write --uncommitted",
    "seed": "ts-node prisma/seed.user.ts",
    "seed:eldevsettings": "ts-node prisma/seed.eldevsettings.ts",
    "seed:aadevsettings": "ts-node prisma/seed.aadevsettings.ts",
    "seed:c2cdevsettings": "ts-node prisma/seed.c2cdevsettings.ts",
    "seed:cvadevsettings": "ts-node prisma/seed.cvadevsettings.ts",
    "seed:rpdevsettings": "ts-node prisma/seed.rpdevsettings.ts",
    "seed:kenyadevsettings": "ts-node prisma/seed.kenyadevsettings.ts",
    "seed:cambodiadevsettings": "ts-node prisma/seed.cambodiadevsettings.ts",
    "seed:chainsettings": "ts-node prisma/seed.chainsettings.ts",
    "seed:setup": "ts-node prisma/seed.settings.ts",
    "seed:contracts": "ts-node ./tools/scripts/local-setup/web3_setup/contractSeed.ts",
    "seed:graph": "ts-node ./tools/scripts/local-setup/web3_setup/graphSeed.ts",
    "seed:settings": "ts-node ./tools/scripts/local-setup/web3_setup/settingsSeed.ts",
    "deploy:prodcontracts": "ts-node ./tools/scripts/production-setup/_setup-deployment.ts",
    "update:prodsettings": "ts-node ./tools/scripts/production-setup/update-deployment.ts",
    "geteth": "ts-node tools/scripts/misc/getEth.ts",
    "graph:codegen": "cd ./apps/graph && graph codegen",
    "graph:build": "cd ./apps/graph && graph build",
    "graph:create-local": "cd ./apps/graph && graph create --node http://localhost:8020/ rahat/core",
    "graph:remove-local": "cd ./apps/graph && graph remove --node http://localhost:8020/ rahat/core",
    "graph:deploy-local": "cd ./apps/graph && graph deploy -l v0.0.1 --node http://localhost:8020/ --ipfs http://localhost:5001 --network mainnet  rahat/core",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "generate:mnemonic": "ts-node ./tools/scripts/misc/mn.ts",
    "contracts:coverage": "cd ./apps/contracts && hardhat coverage",
    "contracts:test": "cd ./libs/contracts && hardhat test",
    "contracts:compile": "cd ./libs/contracts && hardhat compile",
    "test:coverage": "jest --config ./apps/rahat/jest.config.ts --projects ./apps/rahat ./apps/beneficiary --projects ./libs/sdk/jest.config.ts --coverage",
    "test:grievance": "jest --config ./apps/rahat/jest.config.ts ./apps/rahat/src/grievance/grievance.service.spec.ts",
    "test:e2e": "jest --config ./apps/beneficiary-e2e/jest.config.ts",
    "test": "jest",
    "bruno:test": "cd bruno && bru run --env local",
    "bruno:coverage": "cd bruno && bru run --env local --output results.json"
  },
  "private": true,
