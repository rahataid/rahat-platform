meta {
  name: password-status
  type: http
  seq: 9
}

get {
  url: {{url}}/auth/password/status
  body: none
  auth: bearer
}

auth:bearer {
  token: {{access_token}}
}

assert {
  res.status: eq 200
  res.body.success: eq true
  res.body.data: isJson
}

tests {
  test('should return password status', async () => {
    const responseData = res.getBody().data;
    expect(responseData).to.have.property('isPasswordSet');
  });
}
